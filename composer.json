{
    "name": "mileschou/psr",
    "description": "The support library for PSR",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "psr"
    ],
    "authors": [
        {
            "name": "MilesChou",
            "email": "jangconan@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 | ^8 | ^9",
        "psr/container": "^1.0",
        "psr/log": "^1.0",
        "psr/simple-cache": "^1.0",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "MilesChou\\Psr\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
